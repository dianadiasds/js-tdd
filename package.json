{
  "name": "spotify-wrapper",
  "version": "1.0.0",
  "description": "spotify-wrapper",
  "main": "index.js",
  "scripts": {
    "prepush": "npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/main.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/main.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dianadiasds/spotify-wrapper.git"
  },
  "keywords": [
    "spotify"
  ],
  "author": "Diana Dias <dianadiasds@gmail.com> (http://www.dianadias.tech/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dianadiasds/spotify-wrapper/issues"
  },
  "homepage": "https://github.com/dianadiasds/spotify-wrapper#readme",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
  },
  "dependencies": {}
}
